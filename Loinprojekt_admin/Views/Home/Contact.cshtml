@model IEnumerable<Loinprojekt_admin.LoginService.InterfaceAdmin>
@{
    ViewBag.Title = "Contact";
}

<h2 style="color: red; ">@Html.ValidationMessage("Felmeddelande")</h2>

@if (Model.Count() > 0)
{

    <h2>List of administrators</h2>
    <div class="table-wrapper-scroll-y my-custom-scrollbar">
        <table class="table">

            <tr>

                <th>Username</th>
                <th>Email</th>

            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(m => item.Username)</td>
                    <td>@Html.DisplayFor(m => item.Email)</td>
                </tr>
            }
        </table>
    </div>
}
else
{
    <h2 style="color:red">It seems like no administrators have been assigned yet</h2>
}
